    <main class="main">
      <section class="home-slider position-relative pt-25 pb-20">
        <div class="container">
          <div class="row">
            <div class="col-lg-9">
              <div class="position-relative">
                <div
                  class="hero-slider-1 style-3 dot-style-1 dot-style-1-position-1"
                >
                <% banners.forEach(function(banner){ %>
                  <div class="single-hero-slider single-animation-wrap">
                    <div class="container">
                      <div class="slider-1-height-3 slider-animated-1">
                        <div class="hero-slider-content-2">
                          <h4 class="animated"><%=banner.tagLine1%></h4>
                          <h2 class="animated fw-900"><%=banner.mainHeading%></h2>
                          <h1 class="animated fw-900 text-brand">
                            <%=banner.sunHeading%>
                          </h1>
                          <p class="animated">
                            <%=banner.content%>
                          </p>
                          <a
                            class="animated btn btn-brush btn-brush-3"
                            href="/shop"
                          >
                            <%=banner.tagLine2%>
                          </a>
                        </div>
                        <div class="slider-img">
                          <img src="/uploads/<%=banner.Image%>" alt="">
                        

                        </div>
                      </div>
                    </div>
                  </div>
                  <% }) %>
                 
                </div>
                <div class="slider-arrow hero-slider-1-arrow style-3"></div>
              </div>
            </div>
            <div class="col-lg-3 d-md-none d-lg-block">
              <div class="banner-img banner-1 wow fadeIn animated home-3">
                <img
                  class="border-radius-10"
                  src="assets/imgs/banner/banner-5.jpg"
                  alt=""
                />
                <div class="banner-text">
                  <span>Accessories</span>
                  <h4>Save 17% on <br />Autumn Hat</h4>
                  <a href="/shop"
                    >Shop Now <i class="fi-rs-arrow-right"></i
                  ></a>
                </div>
              </div>
              <div class="banner-img banner-2 wow fadeIn animated mb-0">
                <img
                  class="border-radius-10"
                  src="assets/imgs/banner/banner-7.jpg"
                  alt=""
                />
                <div class="banner-text">
                  <span>Smart Offer</span>
                  <h4>Save 20% on <br />Eardrop</h4>
                  <a href="/shop"
                    >Shop Now <i class="fi-rs-arrow-right"></i
                  ></a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
    
      <section class="product-tabs section-padding wow fadeIn animated">
        <div class="container">
          <div class="tab-header">
            <ul class="nav nav-tabs" id="myTab" role="tablist">
              <li class="nav-item" role="presentation">
                <button
                  class="nav-link active"
                  id="nav-tab-one"
                  data-bs-toggle="tab"
                  data-bs-target="#tab-one"
                  type="button"
                  role="tab"
                  aria-controls="tab-one"
                  aria-selected="true"
                >
                  Featured
                </button>
              </li>
              <li class="nav-item" role="presentation">
